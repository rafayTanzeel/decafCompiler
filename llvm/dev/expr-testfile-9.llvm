; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @main() {
entry:
  %x = alloca i32
  store i32 10, i32* %x
  %tmp = load i32, i32* %x
  call void @print_int(i32 %tmp)
  %tmp1 = load i32, i32* %x
  %addtmp = add i32 %tmp1, 10
  store i32 %addtmp, i32* %x
  %tmp2 = load i32, i32* %x
  call void @print_int(i32 %tmp2)
  ret i32 0
}
