; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @main() {
entry:
  %x = alloca i32
  store i32 0, i32* %x
  %tmp = load i32, i32* %x
  call void @print_int(i32 %tmp)
  store i32 12345, i32* %x
  %tmp1 = load i32, i32* %x
  call void @print_int(i32 %tmp1)
  store i32 -12345, i32* %x
  %tmp2 = load i32, i32* %x
  call void @print_int(i32 %tmp2)
  ret i32 0
}
