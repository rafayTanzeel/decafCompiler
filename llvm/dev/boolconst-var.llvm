; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @main() {
entry:
  %b = alloca i1
  store i1 true, i1* %b
  %tmp = load i1, i1* %b
  %zexttmp = zext i1 %tmp to i32
  call void @print_int(i32 %zexttmp)
  store i1 false, i1* %b
  %tmp1 = load i1, i1* %b
  %zexttmp2 = zext i1 %tmp1 to i32
  call void @print_int(i32 %zexttmp2)
  ret i32 0
}
