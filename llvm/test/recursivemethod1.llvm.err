; ModuleID = 'Test'

declare void @print_int(i32)

define i32 @test(i32 %x) {
entry:
  %x1 = alloca i32
  store i32 %x, i32* %x1
  %y = alloca i32
  store i32 10, i32* %y
  %x2 = load i32, i32* %x1
  %y3 = load i32, i32* %y
  %addtmp = add i32 %x2, %y3
  ret i32 %addtmp
}

define i32 @main() {
entry:
  %x = alloca i32
  store i32 1, i32* %x
  %x1 = load i32, i32* %x
  %calltmp = call i32 @test(i32 %x1)
  call void @print_int(i32 %calltmp)
  ret i32 0
}
